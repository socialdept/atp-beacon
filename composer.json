{
    "name": "socialdept/atp-beacon",
    "description": "Resolve AT Protocol DIDs, handles, and lexicon schemas in Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "autoload": {
        "psr-4": {
            "SocialDept\\Beacon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SocialDept\\Beacon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SocialDept\\Beacon\\BeaconServiceProvider"
            ],
            "aliases": {
                "Beacon": "SocialDept\\Beacon\\Facades\\Beacon"
            }
        }
    }
}
